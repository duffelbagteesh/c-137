{% extends "default.njk" %}

{% block main %}
    {% for character in characters %}
    <div class="character-display">
    <a href="/character/{{character.id}}">
        <img src="{{character.image}}" alt="{{character.name}}">
    </a>
    <p>{{character.name}}</p>
    </div>
{% endfor %}
{% endblock %}